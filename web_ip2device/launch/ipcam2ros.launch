<launch>
  <node pkg="web_ip2device" type="wid.py" name="ipcam_to_ros">
  <param name="left_cam_ip" value="192.168.1.8:8080" />
  <param name="right_cam_ip" value="192.168.1.8:8080" />
  <param name="left_config" value="file://$find(web_ip2device)/info/left.yaml" />
  <param name="right_config" value="file://$find(web_ip2device)/info/right.yaml" />
  </node>
</launch>